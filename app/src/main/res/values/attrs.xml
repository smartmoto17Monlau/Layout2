<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontText">
        <attr name="typefaceAsset" format="string"/>
    </declare-styleable>
    <declare-styleable name="Speedometer">
        <attr name="maxSpeed" format="float"></attr>
        <attr name="currentSpeed" format="float"></attr>
        <attr name="onColor" format="color"></attr>
        <attr name="offColor" format="color"></attr>
        <attr name="scaleColor" format="color"></attr>
        <attr name="scaleTextSize" format="dimension"></attr>
        <attr name="readingTextSize" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="Meter">
        <attr name="maxSpeed2" format="float"></attr>
        <attr name="currentSpeed2" format="float"></attr>
        <attr name="scaleTextSize2" format="dimension"></attr>
        <attr name="readingTextSize2" format="dimension"></attr>
    </declare-styleable>
    <declare-styleable name="Marquee">
        <attr name="scaleTextSize3" format="dimension"></attr>
        <attr name="readingTextSize3" format="dimension"></attr>
    </declare-styleable>
</resources>